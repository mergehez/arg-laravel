{
    "version": "0.0.6",
    "name": "mergehez/arg-laravel",
    "description": "Arg-Laravel utilities",
    "keywords": [
        "arg",
        "mergesoft",
        "laravel"
    ],
    "homepage": "https://github.com/mergehez/arg-laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Mazlum Ozdogan",
            "email": "argesozdogan@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "laravel/framework": "^v11.36.1",
        "ext-zip": "*"
    },
    "require-dev": {
        "nunomaduro/collision": "^v8.5.0"
    },
    "autoload": {
        "psr-4": {
            "Arg\\Laravel\\": "src",
            "Arg\\Laravel\\Database\\Factories\\": "database/factories"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {

        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
        }
    },
    "extra": {
        "laravel": {
            "providers": [

            ],
            "aliases": {

            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
